{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    "inline-dotenv",
    "styled-components",
    ["module-resolver", {
      "root": ["./src"]
    }],
    [
      "import",
      {
        "libraryName": "antd",
        "style": "css"
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        ["next/babel", {
           "preset-env": {
              "modules": "commonjs"
            }
        }]
      ]
    }
  }
}
